{
	"author": "Drango",
	"description": "A way to easily create tag functions.",
	"devDependencies": {
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.7.1",
		"typescript": "^4.7.4"
	},
	"license": "MIT",
	"main": "dist/index.js",
	"name": "@drango/tag-functions",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/el-durango/tag-functions.git"
	},
	"scripts": {
		"build": "tsc",
		"format": "prettier --write \"**/*.+(js|ts|json)\"",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"prebuild": "npx rimraf dist",
		"prebuildCmds": "npm run build",
		"preclearCmds": "npm run build",
		"prestart": "npm run build",
		"start": "node ."
	},
	"type": "module",
	"version": "1.0.0"
}
